{
  "manifest_version": 2,
  "name": "notab",
  "version": "1.0.0",
  "description": "notab",
  "icons": {
    "48": "vite.svg"
  },
  "chrome_settings_overrides": {
    "homepage": "index.html"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "browserSettings",
    "cookies",
    "history",
    "menus",
    "storage",
    "tabs",
    "webRequest"
  ],
  "content_security_policy": "default-src 'self';script-src 'self' 'unsafe-eval' 'wasm-unsafe-eval' https://duckduckgo.com; object-src 'self'",
  "browser_specific_settings": {
    "gecko": {
      "id": "notab@usernob",
      "strict_min_version": "115.0"
    }
  }
}
